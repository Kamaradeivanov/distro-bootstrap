alias bssh='ssh -o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null -p 842 -l ibeaute'
alias docker='podman'
alias egpg='export GPG_TTY=$(tty)'
alias gc='git commit -m '
alias gcb='git checkout -b '
alias gfo='git fetch origin'
alias gitlab-runner-local='docker run --rm -it -v "$PWD":"$PWD" --workdir "$PWD" -v /var/run/docker.sock:/var/run/docker.sock gitlab/gitlab-runner exec docker --docker-privileged --docker-pull-policy="always" --docker-volumes /var/run/docker.sock:/var/run/docker.sock'
alias gp='git push'
alias gpf='git push -f'
alias gpsci='git push -o ci.skip'
alias gpsuo='git push --set-upstream origin '
alias grl='git rebase -i HEAD~2'
alias grod='git rebase origin/develop'
alias grom='git rebase origin/master'
alias k='kubectl'
alias kk='k krew'
alias kc='kubecolor'
alias kctx='k ctx'
alias kd='k describe'
alias kg='k get'
alias kgp='k get pods'
alias kl='k logs'
alias kns='k ns'
alias norg='gron --ungron'
alias ohmyzsh="mate ~/.oh-my-zsh"
alias tf='terraform'
alias tfa='tf apply'
alias tfi='tf init'
alias tfp='tf plan'
alias ungron='gron --ungron'
alias watch='watch '
alias wk='watch k'
alias wkg='watch kg'
alias zshconfig="mate ~/.zshrc"
